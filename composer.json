{
    "name": "cybercog/youtrack-php-sdk",
    "description": "PHP YouTrack Software Development Kit.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cybercog",
        "cog",
        "framework-agnostic",
        "laravel",
        "youtrack",
        "yt",
        "sdk",
        "rest",
        "api",
        "client",
        "jetbrains",
        "issues",
        "bugtracker",
        "helpdesk",
        "pm",
        "tickets"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://komarev.com/sources/php-youtrack-sdk",
    "support": {
        "email": "open@cybercog.su",
        "issues": "https://github.com/cybercog/youtrack-php-sdk/issues",
        "wiki": "https://github.com/cybercog/youtrack-php-sdk/wiki",
        "source": "https://github.com/cybercog/youtrack-php-sdk",
        "docs": "https://github.com/cybercog/youtrack-php-sdk/wiki"
    },
    "require": {
        "php": "^7.1|^8.0",
        "cybercog/youtrack-rest-php": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Cog\\YouTrack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\YouTrack\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "cybercog/laravel-youtrack-sdk": "Laravel integration with PHP YouTrack SDK."
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
